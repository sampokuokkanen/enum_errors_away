# frozen_string_literal: true

target :lib do
  signature 'sig'
  signature 'sig/dev' # Development-only type definitions (not distributed with gem)

  check 'lib'

  library 'pathname'
  library 'monitor'
  library 'singleton'
  library 'logger'
  library 'uri'
  library 'time'
  library 'date'
  library 'securerandom'
end
